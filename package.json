{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "sass-build-bulma": "node-sass bulma/bulma.sass bulma/css/bulma.css --source-map bulma/css/bulma.css.map",
    "sass-watch-bulma": "npm run sass-build-bulma -- --watch",
    "sass-start-bulma": "npm run sass-watch-bulma"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "node-sass": "^7.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "bulma": "^0.9.3",
    "sirv-cli": "^2.0.0",
    "svelte-preprocess": "^4.10.4"
  }
}
